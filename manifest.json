{
  "manifest_version": 3,
  "name": "swagger ui viewer",
  "description": "swagger ui viewer",
  "version": "1.0",
  "icons": {
    "128": "src/images/icon-128.png"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_icon": {
      "128": "src/images/icon-128.png"
    }
  },
  "host_permissions": [
    "https://github.com/*"
  ],
  "permissions": ["tabs", "cookies"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+8",
        "mac": "Command+Shift+8"
      }
    }
  }
}
